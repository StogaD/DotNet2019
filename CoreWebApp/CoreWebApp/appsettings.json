{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DescriptionUrl": "https://docs.microsoft.com/en-us/learn/modules/intro-to-logic-apps/index",
  "AllowedHosts": "*",
  "Parameters": {
    "Speed": 20,
    "Acceleration": 20,
    "Name": "f1-22"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "CoreWebApp"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} Added by enricher -> {AppInfo}{NewLine}{Exception}"
        }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "\\Logs\\log-{Date}.txt"
        }
      }
    ],
    "Enrich": [
      "WithAppInfo"
    ]
  },

  "JwtTokenOptions": {
    "Audience": "SampleAudience",
    "Issuer": "JWPAPI",
    "AccessTokenExpiration": 30,
    "RefreshTokenExpiration": 60,
    "SecretKey": "jwts-are-awesome"
  },
  "ConnectionStrings": {
    "CoreWebAppContextConnection": "Server=(localdb)\\mssqllocaldb;Database=CoreWebApp;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}