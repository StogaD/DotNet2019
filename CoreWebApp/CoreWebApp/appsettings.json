{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DescriptionUrl" : "https://docs.microsoft.com/en-us/learn/modules/intro-to-logic-apps/index",
  "AllowedHosts": "*",
  
  "Parameters": {
    "Speed": 20,
    "Acceleration" :20,
    "Name"  : "f1-22"
  },

  "Serilog" : {

    "MinimumLevel": {
      "Default": "Debug",
      "Override" : {
        "System": "Information"
      }
    },
    "WriteTo" : [
      {
        "Name" : "Console"
      },
      {
        "Name" :"RollingFile",
        "Args": {
          "pathFormat": "\\Logs\\log-{Date}.txt"
            }
        }
    ]
  }
}

